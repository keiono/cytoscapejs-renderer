{
  "name": "cytoscapejs-renderer",
  "author": "Keiichiro Ono <kono@ucsd.edu>",
  "version": "v1.0.0",
  "description": "Renderer component for CyNetworkViewer",
  "license": "MIT",
  "keywords": [
    "cytoscape",
    "visualization",
    "graph",
    "bioinformatics"
  ],
  "main": "build/CytoscapeJsRenderer.js",
  "files": [
    "webpack.config.babel.js",
    ".preprocessor.js",
    ".travis.yml",
    ".babelrc",
    "build/*",
    "src/*"
  ],
  "scripts": {
    "build": "./node_modules/.bin/webpack --progress --colors",
    "clean": "rm build/*",
    "lint": "eslint src/; exit 0",
    "test": "eslint src/ && jest",
    "coverage": "jest --coverage"
  },
  "homepage": "https://github.com/CyComponent/CytoscapeJsRenderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/CyComponent/CytoscapeJsRenderer.git"
  },
  "bugs": {
    "url": "https://github.com/CyComponent/CytoscapeJsRenderer/issues"
  },
  "dependencies": {
    "cytoscape": "^3.1.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.1.1",
    "jest": "^20.0.4",
    "webpack": "^3.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/.preprocessor.js"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "es6": true,
      "browser": true
    }
  }
}
