{
    "name": "cytoscapejs-renderer",
    "author": "Keiichiro Ono <kono@ucsd.edu>",
    "version": "v0.1.0",
    "description": "Network renderer components for CyNetworkViewer",
    "license": "MIT",
    "keywords": [
        "cytoscape",
        "visualization",
        "graph",
        "bioinformatics"
    ],
    "main": "build/CytoscapeJsRenderer.js",
    "files": [
        "webpack.config.babel.js",
        ".preprocessor.js",
        ".travis.yml",
        ".babelrc",
        "build/*",
        "src/*"
    ],
    "scripts": {
        "build": "./node_modules/.bin/webpack --progress --colors",
        "clean": "rm build/*",
        "lint": "eslint src/; exit 0",
        "test": "eslint src/ && jest",
        "coverage": "jest --coverage"
    },
    "homepage": "https://github.com/CyComponent/CytoscapeJsRenderer",
    "repository": {
        "type": "git",
        "url": "https://github.com/CyComponent/CytoscapeJsRenderer.git"
    },
    "bugs": {
        "url": "https://github.com/CyComponent/CytoscapeJsRenderer/issues"
    },
    "dependencies": {
        "cytoscape": "^3.2.7",
        "cytoscape-cose-bilkent": "^3.0.4",
        "sigma": "^1.2.1"
    },
    "peerDependencies": {
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "immutable": "^3.8.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.3",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "^4.13.1",
        "eslint-plugin-prettier": "^2.4.0",
        "immutable": "^3.8.2",
        "jest": "^21.2.1",
        "prettier": "1.9.2",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "webpack": "^3.10.0"
    },
    "jest": {
        "scriptPreprocessor": "<rootDir>/.preprocessor.js"
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "extends": "eslint:recommended",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "env": {
            "es6": true,
            "browser": true
        }
    }
}
